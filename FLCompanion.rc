        MENUITEM SEPARATOR
        MENUITEM "E&xit\tEsc",                  IDCANCEL
    END
    POPUP "&View"
    BEGIN
        MENUITEM "Universe &map\tCtrl+M",       ID_SYSTEM_MAP
        MENUITEM "&Base information\tCtrl+B",   ID_BASE_INFO
        MENUITEM "&Items repository\tCtrl+I",   ID_GOODS_REPO
        MENUITEM SEPARATOR
        MENUITEM "Mod information...",          ID_MOD_INFO
