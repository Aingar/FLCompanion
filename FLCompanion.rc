    POPUP "&?"
    BEGIN
        MENUITEM "Online support &forums\tF1",  ID_VISIT_FORUM
        MENUITEM "Visit official &website",     ID_VISIT_WEBSITE
        MENUITEM "View &versions history",      ID_VERSION_HISTORY
